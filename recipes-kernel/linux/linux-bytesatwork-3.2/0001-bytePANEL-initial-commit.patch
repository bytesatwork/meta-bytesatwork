+
+#include <linux/module.h>
+#include <linux/delay.h>
+
+#include <video/omapdss.h>
+
+static struct omap_video_timings emerging_ltp_panel_timings = {
+	.x_res = 800,
+	.y_res = 480,
+
+	.pixel_clock	= 33260,
+
+	.hsw		= 128,
+	.hfp		= 40,
+	.hbp		= 88,
+
+	.vsw		= 2,
+	.vfp		= 8,
+	.vbp		= 27,
+#if 0
+	.hsw		= 20,
+	.hfp		= 10,
+	.hbp		= 216,
+
+	.vsw		= 2,
+	.vfp		= 3,
+	.vbp		= 35,
+#endif
+};
+
+
+static int emerging_ltp_panel_power_on(struct omap_dss_device *dssdev)
+{
+	int r;
+
+	if (dssdev->state == OMAP_DSS_DISPLAY_ACTIVE)
+		return 0;
+
+	r = omapdss_dpi_display_enable(dssdev);
+	if (r)
+		goto err0;
+
+	if (dssdev->platform_enable) {
+		r = dssdev->platform_enable(dssdev);
+		if (r)
+			goto err1;
+	}
+
+	return 0;
+err1:
+	omapdss_dpi_display_disable(dssdev);
+err0:
+	return r;
+}
+
+static void emerging_ltp_panel_power_off(struct omap_dss_device *dssdev)
+{
+	if (dssdev->state != OMAP_DSS_DISPLAY_ACTIVE)
+		return;
+
+	if (dssdev->platform_disable)
+		dssdev->platform_disable(dssdev);
+
+	omapdss_dpi_display_disable(dssdev);
+}
+
+static int emerging_ltp_panel_probe(struct omap_dss_device *dssdev)
+{
+	dssdev->panel.config = OMAP_DSS_LCD_TFT |
+			       OMAP_DSS_LCD_IVS |
+			       OMAP_DSS_LCD_IHS |
+			       OMAP_DSS_LCD_IPC |
+			       OMAP_DSS_LCD_ONOFF;
+
+	dssdev->panel.timings = emerging_ltp_panel_timings;
+
+	return 0;
+}
+
+static void emerging_ltp_panel_remove(struct omap_dss_device *dssdev)
+{
+}
+
+static int emerging_ltp_panel_enable(struct omap_dss_device *dssdev)
+{
+	int r = 0;
+
+	r = emerging_ltp_panel_power_on(dssdev);
+	if (r)
+		return r;
+
+	dssdev->state = OMAP_DSS_DISPLAY_ACTIVE;
+
+	return 0;
+}
+
+static void emerging_ltp_panel_disable(struct omap_dss_device *dssdev)
+{
+	emerging_ltp_panel_power_off(dssdev);
+
+	dssdev->state = OMAP_DSS_DISPLAY_DISABLED;
+}
+
+static int emerging_ltp_panel_suspend(struct omap_dss_device *dssdev)
+{
+	emerging_ltp_panel_power_off(dssdev);
+	dssdev->state = OMAP_DSS_DISPLAY_SUSPENDED;
+	return 0;
+}
+
+static int emerging_ltp_panel_resume(struct omap_dss_device *dssdev)
+{
+	int r = 0;
+
+	r = emerging_ltp_panel_power_on(dssdev);
+	if (r)
+		return r;
+
+	dssdev->state = OMAP_DSS_DISPLAY_ACTIVE;
+
+	return 0;
+}
+
+static struct omap_dss_driver generic_driver = {
+	.probe		= emerging_ltp_panel_probe,
+	.remove		= emerging_ltp_panel_remove,
+
+	.enable		= emerging_ltp_panel_enable,
+	.disable	= emerging_ltp_panel_disable,
+	.suspend	= emerging_ltp_panel_suspend,
+	.resume		= emerging_ltp_panel_resume,
+
+	.driver         = {
+		.name   = "emerging_ltp_panel",
+		.owner  = THIS_MODULE,
+	},
+};
+
+static int __init emerging_ltp_panel_drv_init(void)
+{
+	return omap_dss_register_driver(&generic_driver);
+}
+
+static void __exit emerging_ltp_panel_drv_exit(void)
+{
+	omap_dss_unregister_driver(&generic_driver);
+}
+
+module_init(emerging_ltp_panel_drv_init);
+module_exit(emerging_ltp_panel_drv_exit);
+MODULE_LICENSE("GPL");
