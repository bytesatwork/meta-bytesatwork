 arch/arm/boot/dts/bytepanel-et10.dts          | 131 --------------------------
 arch/arm/boot/dts/bytepanel-etm043080adh6.dts | 131 --------------------------
 2 files changed, 262 deletions(-)
 delete mode 100644 arch/arm/boot/dts/bytepanel-et10.dts
 delete mode 100644 arch/arm/boot/dts/bytepanel-etm043080adh6.dts

diff --git a/arch/arm/boot/dts/bytepanel-et10.dts b/arch/arm/boot/dts/bytepanel-et10.dts
deleted file mode 100644
index 61dc8eb..0000000
--- a/arch/arm/boot/dts/bytepanel-et10.dts
+++ /dev/null
@@ -1,131 +0,0 @@
